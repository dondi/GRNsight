{
    "name": "grnsight",
    "version": "7.5.2",
    "description": "Web app and service for visualizing models of gene regulatory networks",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "body-parser": "1.20.3",
        "canvg": "^3.0.10",
        "cors": "2.8.5",
        "cytoscape": "3.31.0",
        "d3-v4-grid": "2.0.2",
        "dotenv": "16.4.7",
        "errorhandler": "1.5.1",
        "express": "4.21.2",
        "fs": "0.0.1-security",
        "googleapis": "^1.1.5",
        "http": "0.0.0",
        "jspdf": "2.5.2",
        "method-override": "3.0.0",
        "moment": "2.30.1",
        "morgan": "1.10.0",
        "multiparty": "4.2.3",
        "node-xlsx": "0.24.0",
        "nodemon": "3.1.9",
        "pg": "8.13.1",
        "pg-hstore": "2.3.4",
        "pug": "3.0.3",
        "querystring": "0.2.1",
        "save-svg-as-png": "1.4.17",
        "sequelize": "6.37.5",
        "serve-static": "1.16.2",
        "should": "13.2.3",
        "stylus": "0.64.0",
        "supertest": "7.0.0",
        "url": "0.11.4",
        "webpack": "5.97.1",
        "xml2js": "0.6.2",
        "xmlbuilder": "15.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@babel/preset-env": "^7.26.7",
        "@babel/register": "^7.25.9",
        "browser-env": "3.3.0",
        "chai": "^4.3.7",
        "coveralls": "^3.1.1",
        "eslint": "9.19.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "isomorphic-fetch": "3.0.0",
        "jquery": "3.7.1",
        "jquery-extend": "2.0.3",
        "jsdom": "26.0.0",
        "markdown-pdf": "11.0.0",
        "mocha": "^11.1.0",
        "mocha-lcov-reporter": "1.3.0",
        "nock": "14.0.0",
        "npm-run-all2": "^8.0.4",
        "nyc": "^17.1.0",
        "parallelshell": "^3.0.1",
        "prettier": "^3.5.3",
        "sinon": "19.0.2",
        "w3c-xmlserializer": "5.0.0",
        "webpack-cli": "6.0.1",
        "xmldom": "^0.6.0",
        "xmlserializer": "0.6.1"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "scripts": {
        "webpack-start": "webpack --watch",
        "test": "mocha --reporter list --require @babel/register",
        "start-dev": "parallelshell --wait \"nodemon web-client-classic/app.js\" \"nodemon server/app.js\" \"webpack --watch --mode=development\"",
        "start-prod": "parallelshell --wait \"nodemon web-client-classic/app.js\" \"nodemon server/app.js\" \"webpack --watch --mode=production\"",
        "coverage": "nyc --reporter=lcov mocha --require @babel/register",
        "lint": "eslint \"**/*.js\" --ignore-pattern 'web-client/.vite/deps/grommet.js'",
        "coveralls": "npm run coverage && nyc report --reporter=lcov | coveralls",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,json,md}\"",
        "lint:fix": "eslint --fix \"**/*.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dondi/GRNsight.git"
    },
    "keywords": [
        "biojs",
        "GRN",
        "gene regulatory networks",
        "network",
        "visualization",
        "network visualization",
        "force graph visualization"
    ],
    "author": "LMU Bioinformatics Group",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/dondi/GRNsight/issues"
    },
    "homepage": "http://dondi.github.io/GRNsight/"
}
