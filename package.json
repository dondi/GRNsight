{
  "name": "grnsight",
  "version": "4.0.0",
  "description": "Web app and service for visualizing models of gene regulatory networks",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "cors": "2.8.1",
    "cytoscape": "2.7.14",
    "d3-v4-grid": "2.0.1",
    "errorhandler": "1.5.0",
    "express": "4.16.0",
    "fs": "0.0.1-security",
    "googleapis": "1.1.5",
    "http": "0.0.0",
    "jade": "1.11.0",
    "method-override": "2.3.10",
    "moment": "2.17.1",
    "morgan": "1.9.0",
    "multiparty": "4.1.3",
    "node-xlsx": "0.7.2",
    "nodemon": "1.11.0",
    "parallelshell": "3.0.1",
    "serve-static": "1.13.1",
    "should": "11.2.0",
    "stylus": "0.54.5",
    "supertest": "3.0.0",
    "webpack": "4.0.0",
    "xml2js": "0.4.17",
    "xmlbuilder": "8.2.2"
  },
  "devDependencies": {
    "browser-env": "3.2.5",
    "chai": "4.1.2",
    "coveralls": "2.13.1",
    "eslint": "4.18.2",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "0.4.5",
    "jquery": "3.3.1",
    "jquery-extend": "2.0.3",
    "jsdom": "11.11.0",
    "markdown-pdf": "8.0.0",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.3.0",
    "nock": "^10.0.6",
    "sinon": "5.1.1",
    "w3c-xmlserializer": "1.0.1",
    "webpack-cli": "2.0.14",
    "xmlserializer": "0.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "webpack-start": "webpack --watch",
    "test": "mocha --reporter list --compilers js:babel-core/register",
    "start-dev": "parallelshell --wait \"nodemon web-client/app.js\" \"nodemon server/app.js\" \"webpack --watch --mode=development\"",
    "start-prod": "parallelshell --wait \"nodemon web-client/app.js\" \"nodemon server/app.js\" \"webpack --watch --mode=production\"",
    "coverage": "istanbul cover _mocha",
    "lint": "eslint \"**/*.js\"",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dondi/GRNsight.git"
  },
  "keywords": [
    "biojs",
    "GRN",
    "gene regulatory networks",
    "network",
    "visualization",
    "network visualization",
    "force graph visualization"
  ],
  "author": "LMU Bioinformatics Group",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dondi/GRNsight/issues"
  },
  "homepage": "http://dondi.github.io/GRNsight/"
}
