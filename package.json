{
  "name": "grnsight",
  "version": "2.3.0",
  "description": "Web app and service for visualizing models of gene regulatory networks",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "cors": "2.8.1",
    "cytoscape": "2.7.14",
    "express": "3.3.8",
    "googleapis": "1.1.5",
    "jade": "1.11.0",
    "moment": "2.17.1",
    "multiparty": "4.1.3",
    "node-xlsx": "0.7.2",
    "nodemon": "1.11.0",
    "parallelshell": "^3.0.1",
    "should": "11.2.0",
    "stylus": "0.54.5",
    "supertest": "3.0.0",
    "xml2js": "0.4.17",
    "xmlbuilder": "8.2.2"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "istanbul": "0.4.5",
    "jquery-extend": "2.0.3",
    "markdown-pdf": "^8.0.0",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "scripts": {
    "test": "mocha --reporter list",
    "start": "parallelshell --wait \"nodemon web-client/app.js\" \"nodemon server/app.js\"",
    "coverage": "istanbul cover _mocha",
    "lint": "eslint \"**/*.js\"",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dondi/GRNsight.git"
  },
  "keywords": [
    "biojs",
    "GRN",
    "gene regulatory networks",
    "network",
    "visualization",
    "network visualization",
    "force graph visualization"
  ],
